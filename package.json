    "dayjs": "^1.11.9",
    "naive-ui": "^2.34.4",
    "ofetch": "^1.3.3",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.7.0",
    "@bernankez/eslint-config-unocss": "^0.7.0",
